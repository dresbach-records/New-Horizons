---
import CourseListItem from './CourseListItem.astro';

const courses = [
    { id: 1, title: 'Introdução ao Astro.js', students: 500 },
    { id: 2, title: 'Construindo um Blog com Astro', students: 350 },
    { id: 3, title: 'Astro para E-commerce', students: 200 },
];
---

<div class="space-y-4">
    {courses.map(course => <CourseListItem course={course} />)}
</div>